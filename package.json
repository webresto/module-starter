{
  "name": "webresto-module-starter",
  "version": "1.0.0",
  "description": "Starter for sails hook with webpack",
  "main": "index.js",
  "appId": "webresto-module-starter",
  "types": "./src/index.d.ts",
  "scripts": {
    "test": "tsc && mocha -r ts-node/register '{,**/}*.test.ts'",
    "build": "tsc ; browserify -x @webresto/core -x @webresto/graphql/lib/graphqlHelper ./src/initialize.js --s module  --node --bare --no-detect-globals >  dist/module.js"
  },
  "keywords": [],
  "author": "webresto, xziy",
  "license": "ISC",
  "webresto": {
    "moduleSpecificationVersion": "1.0"
  },
  "sails": {
    "isHook": true,
    "hookName": "webresto-module-starter"
  },
  "devDependencies": {
    "@webresto/core": "^0.9.5",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.2",
    "@types/node": "^10.17.60",
    "browserify": "^17.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "faker": "^4.1.0",
    "mocha": "^6.2.2",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typed-sails": "git+https://github.com/42pub/typed-sails.git",
    "typescript": "^3.1.6"
  }
}
